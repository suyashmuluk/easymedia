{
    "name": "easymedia",
    "displayName": "easymedia",
    "description": "This extension helps frontend developer to write the media queries more conviniently",
    "version": "1.0.2",
    "publisher": "msuyash",
    "repository": {
        "type": "git",
        "url": "https://github.com/suyashmuluk/"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "css",
                "path": "./snippets/snippets.code-snippets"
